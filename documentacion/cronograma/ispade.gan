                <depend id="37" type="2" difference="0" hardness="Strong"/>
            </task>
            <task id="37" name="Pruebas de Funcionamiento" color="#99ff99" meeting="false" start="2013-06-07" duration="12" complete="0" expand="true">
                <notes><![CDATA[Se comprueba que lo obtenido sea lo mismo que lo planeado.]]></notes>
                <depend id="40" type="2" difference="0" hardness="Strong"/>
            </task>
            <task id="40" name="Implementar Funciones no estipuladas" color="#ff3333" meeting="false" start="2013-06-21" duration="25" complete="0" priority="4" expand="true">
                <notes><![CDATA[Se implementan funciones que no se estipularon en el momento de diseño del sistema]]></notes>
                <depend id="39" type="2" difference="0" hardness="Strong"/>
            </task>
            <task id="39" name="Distribucion Sistema" color="#00cc00" meeting="false" start="2013-07-20" duration="4" complete="0" expand="true">
                <notes><![CDATA[Se pasa a limpio la documentación, se genera manuales de administrador y de usuario, se procede con el empaquetamiento y distribución del sistema para entornos de producción.]]></notes>
            </task>
        </task>
        <task id="151" name="Correcciones De Problemas" color="#8cb6ce" meeting="false" start="2013-07-24" duration="16" complete="0" expand="true">
